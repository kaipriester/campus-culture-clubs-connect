{
    "routes": [
        {
            "route": "/myclubs/*",
            "allowedRoles": [
                "authenticated"
            ]
        },
        {
            "route": "/mycards/*",
            "allowedRoles": [
                "authenticated", "officer"
            ]
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "*.{css,scss,js,jpg,gif,png,svg,ico}"
        ]
    },
    "responseOverrides": {
        "401": {
            "statusCode": 302,
            "redirect": "/userlogin"
        }
    }
}